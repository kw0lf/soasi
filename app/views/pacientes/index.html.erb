<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>Primer nombre</th>
      <th>Segundo nombre</th>
      <th>Primer apellido</th>
      <th>Segundo apellido</th>
      <th>Tipo identificacion</th>
      <th>Numero identificacion</th>
      <th>Genero</th>
      <th>Fecha nac</th>
      <th>Lugar nacimiento</th>
      <th>Telefono</th>
      <th>Grupo sangre</th>
      <th>Edo civil</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.primer_nombre %></td>
        <td><%= paciente.segundo_nombre %></td>
        <td><%= paciente.primer_apellido %></td>
        <td><%= paciente.segundo_apellido %></td>
        <td><%= paciente.tipo_identificacion %></td>
        <td><%= paciente.numero_identificacion %></td>
        <td><%= paciente.genero %></td>
        <td><%= paciente.fecha_nac %></td>
        <td><%= paciente.lugar_nacimiento %></td>
        <td><%= paciente.telefono %></td>
        <td><%= paciente.grupo_sangre %></td>
        <td><%= paciente.edo_civil %></td>
        <td><%= link_to 'Show', empresa_paciente_path(params[:empresa_id],paciente) %></td>
        <td><%#= link_to 'Edit', edit_paciente_path(paciente) %></td>
        <td><%#= link_to 'Destroy', empresa_paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paciente',new_empresa_paciente_path %>
