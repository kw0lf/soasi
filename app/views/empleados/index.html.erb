<p id="notice"><%= notice %></p>

<h1>Empleados</h1>

<table>
  <thead>
    <tr>
      <th>Nombre1</th>
      <th>Nombre2</th>
      <th>Apellido1</th>
      <th>Apellido2</th>
      <th>Tipo identificacion</th>
      <th>Numero identificacion</th>
      <th>Genero</th>
      <th>Fecha nacimiento</th>
      <th>Lugar nacimiento</th>
      <th>Telefono</th>
      <th>Email</th>
      <th>Edo civil</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empleados.each do |empleado| %>
      <tr>
        <td><%= empleado.nombre1 %></td>
        <td><%= empleado.nombre2 %></td>
        <td><%= empleado.apellido1 %></td>
        <td><%= empleado.apellido2 %></td>
        <td><%= empleado.tipo_identificacion %></td>
        <td><%= empleado.numero_identificacion %></td>
        <td><%= empleado.genero %></td>
        <td><%= empleado.fecha_nacimiento %></td>
        <td><%= empleado.lugar_nacimiento %></td>
        <td><%= empleado.telefono %></td>
        <td><%= empleado.email %></td>
        <td><%= empleado.edo_civil %></td>
        <td><%= link_to 'Show', empleado %></td>
        <td><%= link_to 'Edit', edit_empleado_path(empleado) %></td>
        <td><%= link_to 'Destroy', empleado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empleado', new_empleado_path %>
