<%= simple_form_for(@empleado) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre1 %>
    <%= f.input :nombre2 %>
    <%= f.input :apellido1 %>
    <%= f.input :apellido2 %>
    <%= f.input :tipo_identificacion %>
    <%= f.input :numero_identificacion %>
    <%= f.input :genero %>
    <%= f.input :fecha_nacimiento %>
    <%= f.input :lugar_nacimiento %>
    <%= f.input :telefono %>
    <%= f.input :email %>
    <%= f.input :edo_civil %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
